<%- include('../partials/header') %> 
<header>
    <h1><%= title %> </h1>
</header>
<main>
    <div class="card text-center shadow p-3 mb-5 bg-white rounded" style="width: 30rem;">
        <img src="<%= fish.imageUrl %>" class="card-img-top" alt="fish-image">
        <div class="card-body">
            <h1 class="card-title"><%= fish.species %></h1>
        </div>
</div>

<form method="post" enctype="multipart/form-data" action="/upload">
    <input type="file" name="file">
    <input type="submit" value="Submit">
</form>



<%- include('../partials/footer') %>